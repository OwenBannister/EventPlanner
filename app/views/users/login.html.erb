

    <%= form_tag ('/after_login') do %>
      <%= text_field_tag :email, nil, placeholder: "Email", class: "span4" %>
      <br />
      <%= password_field_tag :password, nil, placeholder: "Password", class: "span4" %>
      <br />
      <%= submit_tag "Login", class: " btn btn-primary btn-large" %>
    <% end %>


